//Write a program to print the following pattern:
5
45
345
2345
12345

#include <stdio.h>

int main() {
    int rows = 5;

    for (int i = rows; i >= 1; i--) {         // Outer loop controls starting number
        for (int j = i; j <= rows; j++) {     // Inner loop prints numbers up to 5
            printf("%d", j);
        }
        printf("\n");                          // Move to next line after each row
    }

    return 0;
}
